{
  "name": "randevubu-server",
  "version": "1.0.0",
  "description": "A basic Node.js server",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:docker": "docker-compose -f docker-compose.dev.yml exec app npm test",
    "test:unit:docker": "docker-compose -f docker-compose.dev.yml exec app npm run test:unit",
    "test:integration:docker": "docker-compose -f docker-compose.dev.yml exec app npm run test:integration",
    "test:coverage:docker": "docker-compose -f docker-compose.dev.yml exec app npm run test:coverage",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed-rbac": "ts-node prisma/seed-rbac.ts",
    "db:seed-subscription-plans": "ts-node prisma/seed-subscription-plans.ts",
    "db:seed-subscription-plans:production": "ts-node scripts/seed-subscription-plans-production.ts",
    "docker:seed-subscription-plans:production": "powershell -ExecutionPolicy Bypass -File scripts/run-seed-subscription-plans-docker.ps1",
    "db:seed-business": "ts-node prisma/seed-business.ts",
    "db:seed-customers": "ts-node prisma/seed-customers-appointments.ts",
    "db:seed-discounts": "ts-node scripts/seed-comprehensive-discount-codes.ts",
    "docker:seed-discounts": "docker-compose exec app npx ts-node scripts/seed-comprehensive-discount-codes.ts",
    "docker:seed-discounts:production": "powershell -ExecutionPolicy Bypass -File scripts/run-seed-discount-codes-docker.ps1",
    "docker:seed-discounts-standalone": "docker-compose -f docker-compose.discount-seeding.yml up --build discount-seeder",
    "test:discounts": "ts-node scripts/simple-discount-test.ts",
    "test:discounts-docker": "docker-compose -f docker-compose.dev.yml exec app npx ts-node scripts/simple-discount-test.ts",
    "test:discounts-validation": "ts-node scripts/discount-validation-test.ts",
    "test:discounts-validation-docker": "docker-compose -f docker-compose.dev.yml exec app npx ts-node scripts/discount-validation-test.ts",
    "test:discounts-complete": "ts-node scripts/test-complete-discount-flow.ts",
    "test:discounts-complete-docker": "docker-compose -f docker-compose.dev.yml exec app npx ts-node scripts/test-complete-discount-flow.ts",
    "test:discounts-flow": "ts-node scripts/test-discount-flow-simple.ts",
    "test:discounts-flow-docker": "docker-compose -f docker-compose.dev.yml exec app npx ts-node scripts/test-discount-flow-simple.ts",
    "db:reset": "prisma migrate reset && npm run db:seed",
    "validate:production": "node scripts/validate-production.js",
    "init:production": "node scripts/init-production.js",
    "migrate:translations": "ts-node src/scripts/migrate-translations.ts",
    "test:translations": "ts-node src/scripts/test-translations.ts",
    "test:sms": "ts-node scripts/test-sms-service.ts",
    "test:sms:diagnostic": "ts-node scripts/test-sms-diagnostic.ts",
    "test:sms:docker": "docker compose -f docker-compose.production.yml exec app1 npx ts-node scripts/test-sms-service.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.883.0",
    "@aws-sdk/client-ses": "^3.917.0",
    "@aws-sdk/lib-storage": "^3.883.0",
    "@aws-sdk/s3-request-presigner": "^3.883.0",
    "@netgsm/sms": "^1.0.0",
    "@prisma/client": "^6.14.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/ioredis": "^4.28.10",
    "@types/joi": "^17.2.2",
    "@types/web-push": "^3.6.4",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dompurify": "^3.0.8",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.1",
    "iyzipay": "^2.0.64",
    "joi": "^18.0.1",
    "jose": "^6.1.3",
    "jsdom": "^24.0.0",
    "libphonenumber-js": "^1.12.12",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "node-cron": "^3.0.3",
    "p-limit": "^4.0.0",
    "prom-client": "^15.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "validator": "^13.15.0",
    "web-push": "^3.6.7",
    "winston": "^3.11.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.13",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^4.17.17",
    "@types/iyzipay": "^2.0.3",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.26",
    "@types/node-cron": "^3.0.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prisma": "^6.14.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
